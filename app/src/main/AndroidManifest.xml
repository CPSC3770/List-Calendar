<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cpsc3770.listcalendar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ViewCalendarActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EditOrCreateEventActivity" />
        <activity android:name=".ViewOrEditCalendarEventActivity" />
        <activity android:name=".SetTimeActivity" />
        <activity android:name=".SetDateActivity" />
        <activity android:name=".ViewEventActivity" />
        <activity android:name=".ColorFilterActivity"></activity>
    </application>

</manifest>